{
  "scripts": {
    "test": "lerna exec --ignore *graphql*  -- jest --coverage",
    "test:watch": "lerna --ignore *graphql*  exec -- jest --watch --coverage",
    "lint": "eslint ./packages/*/src/**/*.js",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna exec --parallel -- rimraf lib dist",
    "build:flow": "lerna exec --parallel --ignore *graphql* -- flow-copy-source -v -i '**/test/**' src lib",
    "build:commonjs": "lerna exec --parallel --ignore *graphql* -- cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files",
    "build:watch": "lerna exec --parallel --ignore *graphql* -- cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files --watch",
    "build:ts": "lerna run --scope canner-graphql-utils build && lerna run --scope canner-graphql-interface build",
    "build": "npm run build:commonjs && npm run build:ts && npm run build:flow",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build",
    "release": "npm run clean && npm run build && lerna publish",
    "release:next": "npm run clean && npm run build && lerna publish --npm-tag next",
    "check:src": "npm run lint",
    "start": "./node_modules/.bin/webpack-dev-server --mode development --config webpack.config.dev.js",
    "build:docs": "./node_modules/.bin/webpack ./docs/index.js --output ./docs/static/bundle.js --mode production --config webpack.config.prod.js",
    "validate": "npm ls"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "7.0.0-rc.1",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-rc.1",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-rc.1",
    "@babel/plugin-proposal-function-sent": "7.0.0-rc.1",
    "@babel/plugin-proposal-json-strings": "7.0.0-rc.1",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-rc.1",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-rc.1",
    "@babel/plugin-syntax-import-meta": "7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-flow": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@canner/antd-components": "^0.4.0-beta.2",
    "antd": "^3.2.3",
    "apollo-link": "^1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "firebase": "^4.13.1",
    "flow-bin": "^0.77.0",
    "flow-copy-source": "^1.3.0",
    "graphiql": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jest": "^23.3.0",
    "lerna": "2.11.0",
    "less": "2.7",
    "less-loader": "^4.0.6",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.7.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "private": true,
  "dependencies": {
    "canner-schema-loader": "^1.3.1",
    "canner-script": "^1.2.7"
  }
}
