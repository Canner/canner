{
  "name": "canner",
  "version": "1.4.4",
  "description": "Build CMS in few lines of code for different data sources",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint ./packages/*/src/**/*.js",
    "clean": "lerna exec --parallel -- rimraf lib dist",
    "build:flow": "lerna exec --parallel -- flow-copy-source -v -i '**/test/**' src lib",
    "build:commonjs": "lerna exec --parallel -- cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files",
    "build:watch": "lerna exec --parallel -- cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files --watch",
    "build": "npm run build:commonjs && npm run build:flow",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build",
    "release": "npm run clean && npm run build && lerna publish --force-publish=*",
    "check:src": "npm run lint",
    "start": "./node_modules/.bin/webpack-dev-server --mode development --config webpack.config.dev.js",
    "build:docs": "./node_modules/.bin/webpack ./docs/index.js --output ./docs/static/bundle.js --mode production --config webpack.config.prod.js",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Canner/canner.git"
  },
  "keywords": [
    "canner",
    "cms",
    "core"
  ],
  "author": "Canner",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Canner/canner/issues"
  },
  "homepage": "https://github.com/Canner/canner#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-flow": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@babel/preset-stage-0": "^7.0.0-beta.52",
    "@canner/antd-components": "^0.3.0-beta.8",
    "antd": "^3.2.3",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.2.0",
    "babel-loader": "^8.0.0-beta",
    "canner-schema-loader": "^1.3.0",
    "canner-script": "^1.2.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-context-patch": "^0.0.6",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "firebase": "^4.13.1",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "graphql": "^0.13.2",
    "jest": "^23.3.0",
    "lerna": "2.11.0",
    "less": "2.7",
    "less-loader": "^4.0.6",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.7.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "apollo-boost": "^0.1.4",
    "bson-objectid": "^1.2.2",
    "canner-compiler": "^0.1.6",
    "canner-graphql-interface": "^1.16.0",
    "canner-helpers": "^0.1.16",
    "canner-layouts": "^1.1.1",
    "canner-ref-id": "^0.1.2",
    "graphql-tag": "^2.9.1",
    "immer": "^1.3.0",
    "immutable": "^3.8.2",
    "invariant": "^2.2.4",
    "lodash": "^4.17.5",
    "lowdb": "^1.0.0",
    "pluralize": "^7.0.0",
    "promise-polyfill": "^7.1.0",
    "prop-types": "^15.6.1",
    "react-apollo": "^2.1.3",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-spinners": "^0.2.6",
    "styled-components": "^3.2.1"
  },
  "peerDependencies": {
    "@canner/antd-components": ">=0.2.0",
    "antd": "^3.2.3",
    "canner-graphql-interface": "^1.16.0",
    "canner-helpers": "^0.1.15",
    "react": ">=16.3",
    "react-dom": ">=16.3"
  }
}
