{
  "name": "@canner/router",
  "version": "0.0.1",
  "description": "Provide a simple router for canner without using react-router",
  "main": "lib/index.js",
  "scripts": {
    "test": "../../node_modules/.bin/jest --coverage",
    "test:watch": "../../node_modules/.bin/jest --watch --coverage"
  },
  "keywords": [
    "canner",
    "router"
  ],
  "author": "Canner",
  "license": "Apache-2.0",
  "dependencies": {
    "@canner/history-router": "^0.0.1"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost"
  }
}
